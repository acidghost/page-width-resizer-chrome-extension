{
  "manifest_version": 2,
 
  "name": "Body Width Resizer",
  "description": "This extension resizes the content page body width.",
  "version": "1.0",

  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
